package = platform.winspool
depends = windows

compilerOpts = \
    -DUNICODE \
    -DWINVER=0x0601 \
    -D_WIN32_WINNT=0x0601 \
    -DWINAPI_FAMILY=3 \
    -DOEMRESOURCE \
    -Wno-incompatible-pointer-types \
    -Wno-deprecated-declarations

headers = windows.h winspool.h
headerFilter = winspool.h
linkerOpts = -lwinspool
